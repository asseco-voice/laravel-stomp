{
    "name": "asseco-voice/laravel-stomp",
    "description": "Laravel Stomp driver",
    "keywords": [
        "php",
        "laravel",
        "driver",
        "stomp",
        "queue"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0",
        "laravel/framework": "^8.0",
        "laravel/horizon": "^5.0",
        "stomp-php/stomp-php": "^4.5",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "mockery/mockery": "^1.3.1",
        "fakerphp/faker": "^1.9.1",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Asseco\\Stomp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asseco\\Stomp\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Asseco\\Stomp\\StompServiceProvider"
            ]
        }
    }
}
